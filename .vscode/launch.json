  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/dist/index.js",
        "stopOnEntry": false,
        "args": [],
        "cwd": "${workspaceRoot}",
        "runtimeArgs": [
          "--nolazy"
        ],
        "sourceMaps": true,
        "outFiles": [ "${workspaceRoot}/dist/**/*.js" ]
        //"trace": "all",
        //"diagnosticLogging": true
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858,
      "sourceMaps": true
    }
  ]
}